{"file":"MainNavigation-BO0G-G3h.js","mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgBA,UAAM,UAAU,IAAI,CAAA,CAAE;AAGS,IAAA,CAAA,QAAA,SAAA,IAAAA,iBAAA,MAAM;AAAA,MAAa;AAAA,MAAW,MAC3D,aAAa,GAAG,EAAE,KAAK,EAAE,kBAAkB,EAAC,CAAE,EAAE,KAAK;AAAA,IACvD,CAAA;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;","names":["_withAsyncContext"],"sources":["../../../../components/global/MainNavigation.vue"],"sourcesContent":["<template>\n  <div>\n    <ul v-for=\"post in folders\" :key=\"post.slug\">\n      <NuxtLink :to=\"post._path\">\n        <li>{{ post.title }}</li>\n      </NuxtLink>\n    </ul>\n    <hr />\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted } from \"vue\";\nimport { useAsyncData } from \"#app\";\n\n// Define a ref for storing folders\nconst folders = ref([]);\n\n// Fetch all posts from both /folders and /page\nconst { data: foldersPosts } = await useAsyncData(\"folders\", () =>\n  queryContent(\"/\").sort({ numbernavigation: 1 }).find()\n);\n\n// Combine and filter the data\nonMounted(() => {\n  const allPosts = [...(foldersPosts.value || [])];\n  if (allPosts.length) {\n    folders.value = allPosts.filter(\n      (post) =>\n        post.included && post.included === true && post.included !== false\n    );\n  }\n});\n</script>\n"],"version":3}