{"file":"PageNavigation-DrSoc8Z2.js","mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAKA,UAAM,oBAAoB,IAAI,EAAE;AAGhC,UAAM,EAAE,KAAM,KAAS,CAAA,QAAA,SAAA,IAAAA,iBAAA,YAAA,aAAa,oBAAoB,YAAY;AAClE,YAAM,UAAU,MAAM,aAAa,OAAO,EAAE,KAAI;AAChD,aAAO;AAAA,IACT,CAAC,CAAA;AAGD,QAAI,KAAK,OAAO;AACd,WAAK,MAAM,QAAQ,UAAQ;AACzB,cAAM,SAAS,KAAK,QAAQ;AAC5B,YAAI,CAAC,kBAAkB,MAAM,MAAM,GAAG;AACpC,4BAAkB,MAAM,MAAM,IAAI,CAAE;AAAA,QAC1C;AACI,0BAAkB,MAAM,MAAM,EAAE,KAAK,IAAI;AAAA,MAC7C,CAAG;AAAA,IACH;;;;;;;;;;;;;;;;;;;;","names":["_withAsyncContext"],"sources":["../../../../components/global/PageNavigation.vue"],"sourcesContent":["<script setup>\nimport { ref } from 'vue';\nimport { useAsyncData } from '#app'; // Ensure correct import for useAsyncData\n\n// Define a reactive reference to store the grouped content files\nconst groupedPagesFiles = ref({});\n\n// Fetch all content files\nconst { data } = await useAsyncData('contentPageFiles', async () => {\n  const content = await queryContent('/page').find(); // Fetch all files and folders\n  return content;\n});\n\n// Group the content files by their folder\nif (data.value) {\n  data.value.forEach(file => {\n    const folder = file._dir || 'Uncategorized';\n    if (!groupedPagesFiles.value[folder]) {\n      groupedPagesFiles.value[folder] = [];\n    }\n    groupedPagesFiles.value[folder].push(file);\n  });\n}\n</script>\n\n<template>\n  <div>\n    <div v-for=\"(files, folder) in groupedPagesFiles\" :key=\"folder\" class=\"pt-2\">\n      <ul class=\"\">\n        <li v-for=\"(file, index) in files\" :key=\"index\">\n          <a :href=\"file._path\">{{ file.title || file._path }}</a>\n        </li>\n      </ul>\n    </div>\n    <hr>\n  </div>\n</template>\n"],"version":3}